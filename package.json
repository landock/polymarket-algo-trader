{
  "name": "@polymarket-algo-trader/chrome-extension",
  "version": "1.0.0",
  "private": true,
  "description": "Polymarket algorithmic trading Chrome extension",
  "scripts": {
    "dev": "cd chrome-extension && npm run dev",
    "build": "cd chrome-extension && npm run build",
    "type-check": "cd chrome-extension && npm run type-check",
